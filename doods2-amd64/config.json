{
  "name": "DOODS2-amd64",
  "version": "1",
  "slug": "doods2-amd64",
  "url": "https://github.com/snowzach/hassio-addons/tree/master/doods2",
  "description": "DOODS V1 Object Detection API - Modern AMD64 Chipsets with stuff.",
  "arch": [
    "amd64"
  ],
  "startup": "services",
  "map": ["share"],
  "usb": true,
  "options": {
    "logger": {
      "level": "info"
    },
    "server": {
      "port": "8080",
      "auth_key": ""
    },
    "doods.detectors": [
      {
        "name": "default",
        "type": "tflite",
        "modelFile": "/opt/doods/models/coco_ssd_mobilenet_v1_1.0_quant.tflite",
        "labelFile": "/opt/doods/models/coco_labels0.txt",
        "hwAccel": false
      },
      {
        "name": "tensorflow",
        "type": "tensorflow",
        "modelFile": "/opt/doods/models/faster_rcnn_inception_v2_coco_2018_01_28.pb",
        "labelFile": "/opt/doods/models/coco_labels1.txt",
        "hwAccel": false
      }
    ]
  },
  "schema": {
    "logger": {
      "level": "str?"
    },
    "server": {
      "port": "port?",
      "auth_key": "str?"
    },
    "doods.detectors": [
      {
        "name": "str",
        "type": "str",
        "modelFile": "str",
        "labelFile": "str?",
        "hwAccel": "bool?"
      }
    ]
  },
  "ports": {
    "8080/tcp": 8080
  },
  "ports_description": {
    "8080/tcp": "DOODS2 port"
  }
}

